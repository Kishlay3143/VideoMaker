<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="oval">
            <padding
                android:bottom="20dp"
                android:left="20dp"
                android:right="20dp"
                android:top="20dp" />
            <stroke
                android:width="4dp"
                android:color="@color/white"
                android:dashWidth="0dp"
                android:dashGap="0dp" />
            <gradient
                android:angle="90"
                android:endColor="#202020"
                android:startColor="#202020" />
            <size
                android:width="20dp"
                android:height="20dp" />
        </shape>
    </item>
</selector>
